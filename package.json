{
  "name": "react-workshop",
  "version": "1.0.0",
  "description": "A simple template for creating react app using TypeScript with hot reload.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --progress --config webpack.prod.config.js",
    "clean": "rd /s/q built",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.ts*\"",
    "prettier:check": "prettier -l \"src/**/*.ts*\"",
    "linter": "tslint -p ./tsconfig.json --fix \"src/**/*.ts*\"",
    "linter:check": "tslint -p ./tsconfig.json \"src/**/*.ts*\"",
    "codefix": "concurrently \"yarn prettier\" \"yarn linter\"",
    "codecheck": "concurrently \"yarn prettier:check\" \"yarn linter:check\""
  },
  "license": "MIT",
  "jest": {
    "testURL": "http://localhost",
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.setup.js",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "jsx",
      "tsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "./tsconfig.jest.json",
        "enableTsDiagnostics": true
      }
    },
    "collectCoverage": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss|svg)$": "identity-obj-proxy",
      "^components(.*)$": "<rootDir>/src/view/components$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^store(.*)$": "<rootDir>/src/store$1",
      "^stylesheets(.*)$": "<rootDir>/src/stylesheets$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^view(.*)$": "<rootDir>/src/view$1",
      "^containers(.*)$": "<rootDir>/src/view/containers$1",
      "^routes(.*)$": "<rootDir>/src/view/routes$1",
      "^templates(.*)$": "<rootDir>/src/view/templates$1"
    },
    "moduleDirectories": [
      "node_modules",
      "main/node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn codecheck",
      "pre-push": "yarn codecheck"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "i18next": "^11.4.0",
    "i18next-xhr-backend": "^1.5.1",
    "lodash": "^4.17.10",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "qs": "^6.5.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "^4.3.4",
    "react-i18next": "^7.9.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^4.0.8",
    "react-select": "^2.0.0",
    "redux": "4.0.0",
    "redux-form": "^7.4.2",
    "redux-form-saga": "^0.2.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^6.0.0",
    "tiny-cookie": "^2.1.2"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.5",
    "@types/enzyme": "^3.1.12",
    "@types/expect": "^1.20.3",
    "@types/i18next": "^8.4.4",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.112",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-i18next": "^7.8.0",
    "@types/react-redux": "6.0.2",
    "@types/react-router": "4.0.26",
    "@types/react-router-config": "1.0.7",
    "@types/react-router-dom": "4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/react-select": "^2.0.2",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-form": "^7.4.3",
    "@types/redux-mock-store": "^1.0.0",
    "autoprefixer": "^9.0.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "i18next-resource-store-loader": "^0.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "jest-enzyme": "^6.0.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.13.7",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "2.13.4",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.0",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "typescript": "3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.4",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
